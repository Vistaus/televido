<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="https://gitlab.gnome.org/GNOME/glib/-/raw/main/gio/gschema.dtd"?>
<schemalist gettext-domain="televido">
	<schema id="de.k_bo.Televido" path="/de/k_bo/televido/">
		<key name="width" type="i">
			<default>720</default>
		</key>
		<key name="height" type="i">
			<default>480</default>
		</key>
		<key name="is-maximized" type="b">
			<default>false</default>
		</key>
		<key name="is-fullscreen" type="b">
			<default>false</default>
		</key>
		<key name="visible-view" type="s">
			<default>"live"</default>
		</key>
		<key name="search-in-topic" type="b">
			<default>true</default>
		</key>
		<key name="search-in-title" type="b">
			<default>true</default>
		</key>
		<key name="search-in-description" type="b">
			<default>false</default>
		</key>
		<key name="search-in-channel" type="b">
			<default>false</default>
		</key>
		<key name="sort-by" type="s">
			<default>"date"</default>
		</key>
		<key name="sort-order" type="s">
			<default>"descending"</default>
		</key>
		<key name="video-player-name" type="s">
			<default>""</default>
		</key>
		<key name="video-player-id" type="s">
			<default>""</default>
		</key>
		<key name="video-downloader-name" type="s">
			<default>""</default>
		</key>
		<key name="video-downloader-id" type="s">
			<default>""</default>
		</key>
		<key name="default-playback-quality" type="s">
			<default>"high"</default>
		</key>
	</schema>
</schemalist>